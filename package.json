{
  "name": "expense-tracker",
  "version": "1.0.0",
  "description": "HackerYou Fullstack Masterclass Project",
  "main": "index.js",
  "engines": {
    "node": "13.0.1",
    "npm": "6.12.0"
  },
  "scripts": {
    "build": "webpack --mode production --config webpack.server.config.js",
    "clean:server": "rimraf server-dist && mkdir -p server-dist",
    "dev": "npm-run-all dev:server dev:client",
    "dev:server": "npm-run-all --parallel webpack:server server",
    "webpack:server": "webpack --config webpack.server.config.js --watch",
    "start": "node server.js",
    "server": "nodemon --exec \"node server.js\" --ignore src/ --ignore build/ --ignore public/",
    "dev:client": "npm run start --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [],
  "author": "Marc Brathwaite",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "eslint": "^6.6.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.21.2",
    "hpp": "^0.2.2",
    "init-package-json": "^1.10.3",
    "jsonwebtoken": "^8.5.1",
    "lodash.tointeger": "^4.0.4",
    "lodash.tonumber": "^4.0.3",
    "mongoose": "^5.7.10",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.19.4",
    "validator": "^12.1.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "standard": "^12.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
